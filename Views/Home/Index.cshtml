@model IEnumerable<CommunityAssist2017.Models.GrantType>

@{
    ViewBag.Title = "Home Page";
}
<h1>Our Grants are great!</h1>
<table class="table">
    <thead>
        <tr>
            <th scope = "col">Type</th>
            <th scope="col">Description</th>
            <th scope="col">One-Time Max</th>
            <th scope="col">Lifetime Max</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(model =>item.GrantTypeName)</td>
                <td>@Html.DisplayFor(model =>item.GrantTypeDescription)</td>
                <td>@Html.DisplayFor(model =>item.GrantTypemaximum)</td>
                <td>@Html.DisplayFor(model =>item.GrantTypeLifetimeMaximum)</td>
            </tr>
        }
    </tbody>

</table>